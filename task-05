    <script>
        const promise1 = new Promise((resolve, reject) => {
            setTimeout(() => {
                resolve("First Promise won! (Resolved in 1 second)");
            }, 1000);
        });

        const promise2 = new Promise((resolve, reject) => {
            setTimeout(() => {
                resolve("Second Promise won! (Resolved in 3 seconds)");
            }, 3000);
        });

        Promise.race([promise1, promise2])
            .then(winner => {
                console.log(winner);
            })
            .catch(error => {
                console.log(error);
            });
    </script>
