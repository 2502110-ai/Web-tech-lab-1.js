        const buffer = new ArrayBuffer(16);
        const view = new Uint8Array(buffer);
        for (let i = 0; i < 16; i++) {
            view[i] = 65 + i; 
        }
        const decoder = new TextDecoder();
        const decodedString = decoder.decode(view);
        console.log(decodedString);
    </script>
